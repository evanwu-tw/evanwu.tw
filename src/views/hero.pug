        div(class="parallax-container" data-depth="0.6")
          svg(class="square square--about" width="57px" height="57px" viewBox="0 0 57 57")
            g(stroke="none" stroke-width="1" fill="none" fill-rule="evenodd")
              g(transform="translate(-1193.000000, -578.000000)" fill="#4ACDD9")
                polygon(points="1193.59798 578.59798 1249.59798 578.59798 1249.59798 634.59798 1193.59798 634.59798")
        div(class="parallax-container" data-depth="0.3")
      .hero__inner
        .hero-title
          h1 Better design,<br>Better experiences.
      .scroll-tip
        i.fas.fa-chevron-down
