          svg(class="square-line-2 square-line-2--index" width="119px" height="119px" viewBox="0 0 119 119")
            g(stroke="none" stroke-width="1" fill="none" fill-rule="evenodd")
              g(transform="translate(-1181.000000, -829.000000)" stroke="#F7F7F7" stroke-width="15")
