case hero
  //- index
  when "about"
    .hero.hero__sub
      .hero-bg
        div(class="parallax-container" data-depth="1")
          svg(class="square-line-2 square-line-2--about" width="119px" height="119px" viewBox="0 0 119 119")
            g(stroke="none" stroke-width="1" fill="none" fill-rule="evenodd")
              g(transform="translate(-1181.000000, -829.000000)" stroke="#F7F7F7" stroke-width="15")
                path(d="M1188.68466,837.395371 L1291.68466,837.395371 L1291.68466,940.395371 L1188.68466,940.395371 L1188.68466,837.395371 Z")
        div(class="parallax-container" data-depth="0.8")
          svg(class="square-line-3 square-line-3--about" width="465px" height="465px" viewBox="0 0 465 465")
            g(stroke="none" stroke-width="1" fill="none" fill-rule="evenodd")
              g(transform="translate(-2079.000000, -248.000000)" stroke="#F7F7F7" stroke-width="30")
                path(d="M2094.16227,263.503308 L2528.16227,263.503308 L2528.16227,697.503308 L2094.16227,697.503308 L2094.16227,263.503308 Z")
